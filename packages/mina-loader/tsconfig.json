{
  "compilerOptions": {
    "outDir": "./lib",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "listEmittedFiles": true,
    "listFiles": true,
    "allowJs": true,
    "target": "es5"
  },
  "include": ["./src/**/*"]
}
